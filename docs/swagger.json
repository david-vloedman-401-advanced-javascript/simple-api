{
  "openapi": "3.0.1",
  "info": {
    "title": "Core API",
    "description": "The worlds funnest little API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000/"
    }
  ],
  "paths": {
    "/categories": {
      "get": {
        "description": "Get all categories",
        "responses": {
          "200": {
            "description": "Returned a list of categories"
          }
        }
      },
      "post": {
        "description": "post a category",
        "responses": {
          "200": {
            "description": "Successful post"
          }
        }
      },
      "put": {
        "description": "put a category",
        "responses": {
          "200": {
            "description": "Successful put"
          }
        }
      },
      "delete": {
        "description": "Delete a category",
        "responses": {
          "200": {
            "description": "Deleted a category"
          }
        }
      }
    }
  }
  "paths": {
    "/product": {
      "get": {
        "description": "Get all products",
        "responses": {
          "200": {
            "description": "Returned a list of products"
          }
        }
      },
      "post": {
        "description": "Post a product",
        "responses": {
          "200": {
            "description": "Posted a product"
          }
        }
      },
      "put": {
        "description": "Put a product",
        "responses": {
          "200": {
            "description": "Put a product"
          }
        }
      },
      "delete": {
        "description": "Delete a product",
        "responses": {
          "200": {
            "description": "Delete a product"
          }
        }
      }
    }
  }

}
